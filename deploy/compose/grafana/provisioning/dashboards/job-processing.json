{
  "title": "Job Processing",
  "uid": "cloudtask-job-processing",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": { "from": "now-1h", "to": "now" },
  "timezone": "browser",
  "tags": ["cloudtask"],
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Job Creation Rate (by type)",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": []
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "rate(jobs_created_total[5m])",
          "legendFormat": "{{type}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Job Creation Latency (p50 / p95 / p99)",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.50, rate(jobs_created_seconds_bucket[5m]))",
          "legendFormat": "p50 {{type}}",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.95, rate(jobs_created_seconds_bucket[5m]))",
          "legendFormat": "p95 {{type}}",
          "refId": "B"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.99, rate(jobs_created_seconds_bucket[5m]))",
          "legendFormat": "p99 {{type}}",
          "refId": "C"
        }
      ],
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Worker: Jobs Processed Rate (by type / status)",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": []
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "rate(worker_jobs_processed_total[5m])",
          "legendFormat": "{{type}} / {{status}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Worker: Job Duration p99 (by type)",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.99, rate(worker_job_duration_seconds_bucket[5m]))",
          "legendFormat": "p99 {{type}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Processor: Status Update Rate (by status)",
      "gridPos": { "x": 0, "y": 16, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": []
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "rate(processor_status_updates_total[5m])",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "RabbitMQ Publish Errors",
      "gridPos": { "x": 12, "y": 16, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": { "unit": "reqps", "color": { "fixedColor": "red", "mode": "fixed" } },
        "overrides": []
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "rate(rabbitmq_publish_total{outcome=\"failure\"}[5m])",
          "legendFormat": "publish failures",
          "refId": "A"
        }
      ],
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    }
  ]
}
